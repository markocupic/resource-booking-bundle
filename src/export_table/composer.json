{
  "name": "markocupic/export_table",
  "description": "Export Contao tables to csv- or xml-file.",
  "keywords": [
    "contao",
    "export table"
  ],
  "type": "contao-module",
  "license": "MIT",
  "authors": [
    {
      "name": "Marko Cupic",
      "email": "m.cupic@gmx.ch",
      "homepage": "https://github.com/markocupic",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/markocupic/export_table/issues",
    "source": "https://github.com/markocupic/export_table"
  },
  "require": {
    "contao/core-bundle": "^4.5",
    "league/csv": "^9.1"
  },
  "replace": {
    "contao-legacy/export_table": "*"
  },
  "autoload": {
    "psr-4": {
      "Markocupic\\ExportTable\\": "src/"
    },
    "classmap": [
      "src/Resources/contao"
    ],
    "exclude-from-classmap": [
      "src/Resources/contao/config",
      "src/Resources/contao/dca",
      "src/Resources/contao/languages"
    ]
  },
  "extra": {
    "contao-manager-plugin": "Markocupic\\ExportTable\\ContaoManager\\Plugin",
    "contao": {
      "sources": {
        "": "system/modules/export_table"
      }
    }
  }
}

